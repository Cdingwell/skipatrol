	</div>

	<div class="node">
		<div class="title">Instructor ID</div>
		<input class="InstID" value="{{InstID}}">
	</div>

	<div class="node">
		<div class="title">Name</div>
		<input class="Name" value="{{Name}}">
	</div>

	<div class="node">
		<div class="title">Email</div>
		<input class="Email" value="{{Email}}">
	</div>

	<div class="node">
